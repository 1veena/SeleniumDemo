<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="paralleltesting" parallel="methods">
	<test thread-count="5" name="ChrometTest">
		<parameter name="browser" value="Chrome"></parameter>
		<classes>
			<class name="testScript.LoginTest">
				<methods>
					<include
						name="verifytheUserisabletoLoginUsingValidCredentials"></include>
					<include
						name="verifytheUserisabletoLoginUsingValidUsernameInValidPassword"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite> 
